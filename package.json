{
  "name": "@craigmorrison/eslint-config",
  "version": "0.6.1",
  "description": "Craig Morrison's personal ESLint config",
  "author": "Craig Morrison",
  "license": "MIT",
  "homepage": "https://github.com/craigmorrison/eslint-config-craigmorrison#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigmorrison/eslint-config-craigmorrison.git"
  },
  "bugs": {
    "url": "https://github.com/craigmorrison/eslint-config-craigmorrison/issues"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "react.js"
  ],
  "keywords": [
    "ESLint",
    "config"
  ],
  "scripts": {
    "build": "node ./config/build.mjs",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "quoteProps": "consistent"
  },
  "devDependencies": {
    "@craigmorrison/eslint-config": "^0.1.0",
    "eslint-find-rules": "^3.6.1",
    "prettier": "^2.3.0"
  },
  "peerDependencies": {
    "eslint": "^7.27.0"
  },
  "dependencies": {
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  }
}
